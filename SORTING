BUBBLE SORT
void bubble sort(int arr[],int n){
for(int i=n-1;i>=0;i--){
int didSwap=0;
for(int j=0;j<=i-1;j++){
if(arr[j]>arr[j+1]){
int temp=arr[j];
arr[j]=temp;
didSwap=1;
]
]
if(didSwap ==0){
break;

INSERTION SORT
void insertion sort(int arr[],int n){
for(int i=0;i<=n-1;i++){
int j=i;
while(j>0 && arr[j-1]>arr[j]){
int temp=arr[j-1];
arr[j-1]=arr[j];
arr[j]=temp;
j--;
}
}
